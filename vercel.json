{
    "builds": [
      {
        "src": "app.py",
        "use": "@now/python"
      }
    ],
      "routes": [
      { "src": "/administracion", "dest": "/administracion.html" },
      { "src": "/comentarios", "dest": "/comentarios.html" },
      { "src": "/", "dest": "/index.html" },
      { "src": "/login", "dest": "/login.html" },
      { "src": "/register", "dest": "/register.html" },
      { "src": "/solicitud", "dest": "/solicitud.html" }
    ],
    "env": {
      "MONGO_URI": "mongodb://localhost:27017/app",
      "SECRET_KEY": "secretkey",
      "MONGO_DBNAME": "app",
      "PORT": "5000"
    }
  }
  